{
  "name": "Shprenger.pro",
  "author": "Roman Shprenger",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "1.0.17"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "bootstrap": "^4.0.0-beta.3",
    "html-react-parser": "^0.4.1",
    "loaders.css": "^0.1.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loaders": "^3.0.1",
    "react-reveal": "^0.7.3",
    "react-scroll": "^1.7.4",
    "reactstrap": "^5.0.0-alpha.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --inlude-path ./src --inlude-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --inlude-path ./src --inlude-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
